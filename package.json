{
  "name": "gridworld",
  "version": "1.0.0",
  "description": "GridWorld - Interactive 3D Hexasphere",
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "webpack serve --mode development --stats minimal",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development --stats minimal",
    "build:watch": "webpack --mode development --watch --stats minimal",
    "clean": "rimraf dist",
    "start": "npm run dev",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "tauri": "tauri"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@tauri-apps/cli": "^2.10.0",
    "@types/node": "^25.1.0",
    "@types/three": "^0.182.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.57.0",
    "html-webpack-plugin": "^5.5.4",
    "mini-css-extract-plugin": "^2.9.2",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.3"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.10.1",
    "three": "^0.164.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/arscan/hexashere.js.git"
  },
  "keywords": [
    "3d",
    "hexasphere",
    "three.js",
    "gridworld",
    "tauri"
  ],
  "author": "robscanlon@gmail.com",
  "license": "MIT"
}
